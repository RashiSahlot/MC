//Switch-case program
#include<stdio.h>
int main()
{
	int opt;
	printf("Enter any number from 1 to 5: ");
	scanf("%d",&opt);
	switch (opt)
	{
		case 1: printf("Food item- French Fries\nPrice- Rs.99");
		break;
		case 2: printf("Food item- Pizza\nPrice- Rs.239");
		break;  
		case 3: printf("Food item -Sandwhich\nPrice- Rs.149");
		break;
		case 4: printf("Food item- Burger\nPrice- Rs.129");
		break;
		case 5: printf("Food item- Pasta\nPrice- Rs.179");
		break;
	    default: printf("U entered wrong number, try again.");
    }
    return 0;
}
